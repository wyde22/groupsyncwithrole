<?xml version="1.0"?>
<extension key="groupsyncwithrole" type="module">
  <file>groupsyncwithrole</file>
  <name>Group Sync with WordPress Role</name>
  <description>This extension manages the synchronization between CiviCRM groups and the WordPress role.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Dewy</author>
    <email>dewymercerais@gmail.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://FIXME</url>
    <url desc="Documentation">http://FIXME</url>
    <url desc="Support">http://FIXME</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-08-17</releaseDate>
  <version>1.0.0</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.73</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Groupsyncwithrole</namespace>
    <format>23.02.1</format>
    <angularModule>crmGroupsyncwithrole</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
    <mixin>menu-xml@1.0.0</mixin>
  </mixins>
</extension>
